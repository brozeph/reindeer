{
  "name": "reindeer",
  "description": "Provides schema enforcement based on Elasticsearch type mappings",
  "main": "./dist",
  "version": "3.2.2",
  "author": "Joshua Thomas (https://github.com/brozeph)",
  "contributors": [
    "Robin Momii (https://github.com/rmomii)"
  ],
  "engines": {
    "node": ">= 6.0"
  },
  "keywords": [
    "elasticsearch",
    "mapping",
    "schema",
    "validation"
  ],
  "repository": "git://github.com/brozeph/reindeer.git",
  "homepage": "http://github.com/brozeph/reindeer",
  "nyc": {
    "all": true,
    "exclude": [
      "src/index.js"
    ],
    "include": [
      "src"
    ],
    "instrument": false,
    "report-dir": "./reports",
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "require": [
      "@babel/register"
    ],
    "sourceMap": false
  },
  "license": "MIT",
  "scripts": {
    "lint": "gulp lint",
    "posttest": "nyc report --reporter=text-lcov | coveralls",
    "prepare": "gulp build",
    "pretest": "gulp clean && gulp lint",
    "test": "NODE_ENV=test nyc mocha ./test/src",
    "test:unit": "NODE_ENV=test nyc mocha ./test/src"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/register": "^7.11.5",
    "babel-eslint": "^10.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "eslint-plugin-babel": "^5.3.1",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-eslint": "^6.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "mocha": "^8.1.3",
    "nock": "^13.0.4",
    "nyc": "^15.1.0",
    "uuid": "^8.3.1"
  },
  "dependencies": {
    "@babel/runtime-corejs2": "^7.11.2",
    "es": "^0.7.3"
  }
}
